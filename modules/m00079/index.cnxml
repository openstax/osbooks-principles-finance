  <title>Measures of Spread</title>
  <metadata xmlns:md="http://cnx.rice.edu/mdml">
    <md:title>Measures of Spread</md:title>
    <md:content-id>m00079</md:content-id>
    <md:uuid>97a226f5-360b-4081-af5d-5a765fe6b1a0</md:uuid>
