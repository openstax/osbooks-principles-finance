            <entry align="center">6</entry>
            <entry align="center">7</entry>
          </row>
        </thead>
        <tbody>
          <row>
            <entry align="left">Cash Invested</entry>
            <entry align="right"/>
            <entry align="right">$2,000</entry>
            <entry align="right">$2,000</entry>
            <entry align="right">$2,000</entry>
            <entry align="right">$12,000</entry>
